{
  "name": "inavord-react-router",
  "description": "An opinionated React Router v7 template, Tailwind v4, Netlify deployment, vite build, with Supabase authentication and authorization",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "react-router build && node netlify/prepare.js",
    "dev": "cross-env NODE_ENV=development node ./dev-server.js",
    "start": "netlify serve",
    "tsc": "react-router typegen && tsc",
    "supabase:types": "supabase gen types typescript --local > app/types/supabase.ts"
  },
